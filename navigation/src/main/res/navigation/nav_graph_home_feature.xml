<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_home_feature"
    app:startDestination="@id/login_fragment"
    tools:ignore="MissingDefaultResource">

    <fragment android:id="@+id/login_fragment"
        android:name="com.stockbit.accounts.presentation.login.LoginFragment"
        tools:layout="@layout/accounts_login_fragment">
        <action
            android:id="@+id/action_to_home"
            app:destination="@+id/home_fragment"/>
    </fragment>

    <fragment
        android:id="@+id/home_fragment"
        android:name="com.stockbit.home.presentation.HomeFragment"
        tools:layout="@layout/fragment_home" />

</navigation>